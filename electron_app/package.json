{
  "name": "electron_app",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "devDependencies": {
    "electron-builder": "^22.9.1",
    "electron": "^11.1.1"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64"
  },
  "build": {
    "appId": "testJC",
    "asar": true,
    "directories": {
      "output": "build"
    },
    "files": [
      "**/*"
    ],
    "compression": "store",
    "win": {
      "target": "portable"
    }
  }
}
