{
  "name": "client_app",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "devDependencies": {
    "electron-builder": "22.11.2",
    "electron": "12.0.7"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64"
  },
  "build": {
    "appId": "testJC",
    "asar": true,
    "directories": {
      "output": "../build/client-app"
    },
    "files": [
      "**/*"
    ],
    "compression": "store",
    "win": {
      "target": "portable"
    }
  }
}

