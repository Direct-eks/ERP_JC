{
    "name": "frontend",
    "version": "0.0.1",
    "author": "dw",
    "description": "frontend jc using vue",
    "browserslist": [
        "last 2 Chrome major versions",
        "last 2 Electron major versions"
    ],
    "engines": {
        "node": ">=12"
    },
    "private": true,
    "scripts": {
        "dev": "webpack serve --env BUILD_ENV=development --config webpack.dev.config.js",
        "buildElectron": "rimraf ../electron_app/webpages && webpack --progress --env BUILD_ENV=production --config webpack.prod.config.js",
        "buildWeb": "rimraf ../backend/src/main/resources/static && webpack --progress --env BUILD_ENV=webOnly --config webpack.prod.config.js"
    },
    "dependencies": {
        "@mdi/js": "5.9.55",
        "axios": "0.21.1",
        "big.js": "6.1.1",
        "vue": "2.6.14",
        "vue-router": "3.5.2",
        "vuetify": "2.5.8",
        "vuetify-loader": "1.7.2",
        "vuex": "3.6.2"
    },
    "devDependencies": {
        "@babel/core": "^7.14.2",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.14.2",
        "@babel/preset-env": "^7.14.2",
        "@babel/runtime": "^7.14.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "css-loader": "^5.2.4",
        "deepmerge": "^4.2.2",
        "eslint": "^7.26.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.23.1",
        "eslint-plugin-vue": "^7.9.0",
        "fibers": "^5.0.0",
        "html-webpack-plugin": "5.3.2",
        "mini-css-extract-plugin": "2.2.0",
        "sass": "~1.32.13",
        "sass-loader": "12.1.0",
        "style-loader": "3.2.1",
        "url-loader": "4.1.1",
        "vue-loader": "15.9.8",
        "vue-template-compiler": "2.6.14",
        "webpack": "5.51.1",
        "webpack-bundle-analyzer": "4.4.2",
        "webpack-cli": "4.8.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "5.8.0"
    }
}
